[nodes]
node1 ansible_host=node1 ansible_user=ansible ansible_ssh_private_key_file=./ssh/id_rsa



[webservers]
web1 ansible_host=webserver1 ansible_user=ansible ansible_ssh_private_key_file=./ssh/id_rsa

[dockers]
docker ansible_host=docker ansible_user=ansible ansible_ssh_private_key_file=./ssh/id_rsa


[jenkins_servers]
ec2-instance ansible_host=<EC2_PUBLIC_IP> ansible_user=ubuntu ansible_ssh_private_key_file=<PATH_TO_EC2_KEY>/<KEY_NAME>.pem


[redis_servers]
ec2-instance ansible_host=<EC2_PUBLIC_IP> ansible_user=ubuntu ansible_ssh_private_key_file=/path/to/your-key.pem


[postgresql_servers]
ec2-instance1 ansible_host=<EC2_PUBLIC_IP_1> ansible_user=ubuntu ansible_ssh_private_key_file=/path/to/your-key.pem
ec2-instance2 ansible_host=<EC2_PUBLIC_IP_2> ansible_user=ubuntu ansible_ssh_private_key_file=/path/to/your-key.pem
